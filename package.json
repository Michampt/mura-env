{
  "name": "mura-env",
  "displayName": "Mura Configuration Helper",
  "description": "Hover documentation for Mura environment variables and settings",
  "version": "0.2.4",
  "license": "SEE LICENSE IN LICENSE",
  "publisher": "michampt",
  "licenser": {
    "author": "Michael Hampton"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Michampt/mura-env"
  },
  "engines": {
    "vscode": "^1.57.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:yml",
    "onLanguage:ini",
    "onLanguage:yaml",
    "onStartupFinished"
  ],
  "main": "src/extension.js",
  "contributes": {
    "commands": [],
    "capabilities": {
      "hoverProvider": "true"
    },
    "languages": [
      {
        "id": "yml",
        "extensions": [
          ".yml",
          ".yaml"
        ]
      },
      {
        "id": "ini",
        "extensions": [
          ".ini",
          ".ini.cfm"
        ]
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.57.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}
